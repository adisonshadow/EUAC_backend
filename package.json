{
  "name": "uac-api",
  "version": "1.0.7",
  "description": "UAC REST API Service",
  "main": "src/app.js",
  "scripts": {
    "dev": "NODE_ENV=development node src/app.js",
    "dev:watch": "NODE_ENV=development nodemon src/app.js",
    "test": "jest",
    "lint": "eslint src/",
    "test-with-reset-db": "cross-env RESET_DB=true jest --config=jest.config.js",
    "init-db": "./scripts/initdb.sh",
    "init-db-with-mock": "./scripts/initdb.sh --with-mock",
    "db:maintenance": "bash scripts/db-maintenance.sh",
    "db:vacuum": "bash scripts/db-maintenance.sh VACUUM",
    "db:analyze": "bash scripts/db-maintenance.sh ANALYZE",
    "db:reindex": "bash scripts/db-maintenance.sh REINDEX",
    "db:maintenance:all": "bash scripts/db-maintenance.sh ALL",
    "db:backup": "bash scripts/db-backup.sh",
    "db:backup:setup-cron": "bash scripts/setup-backup-cron.sh",
    "swagger": "node scripts/generate-swagger.js"
  },
  "dependencies": {
    "@koa/cors": "^5.0.0",
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.3.1",
    "eslint": "^9.27.0",
    "jsonwebtoken": "^9.0.2",
    "koa": "^2.14.2",
    "koa-body": "^6.0.1",
    "koa-bodyparser": "^4.4.1",
    "koa-jwt": "^4.0.4",
    "koa-multer": "^1.0.2",
    "koa-router": "^12.0.1",
    "koa2-swagger-ui": "^5.11.0",
    "pg": "^8.15.6",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.7",
    "sharp": "^0.34.2",
    "swagger-jsdoc": "^6.2.8",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.27.0",
    "@types/jest": "^29.5.14",
    "cross-env": "^7.0.3",
    "eslint-plugin-react": "^7.37.5",
    "faker": "^6.6.6",
    "globals": "^16.2.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "supertest": "^7.1.0"
  }
}
